# Configuration pour l'environnement de développement Python 3.12
# Fichier .env_312 pour les notebooks Jupyter

# === API KEYS ===
# Clé API OpenAI (obligatoire)
OPENAI_API_KEY=your_openai_api_key_here

# Clé API Tavily pour la recherche web (optionnel)
TAVILY_API_KEY=your_tavily_api_key_here

# === CONFIGURATION QDRANT ===
# URL du serveur Qdrant (local par défaut)
QDRANT_URL=http://localhost:6333

# Clé API Qdrant (optionnel pour instance locale)
QDRANT_API_KEY=

# === CONFIGURATION MODÈLES ===
# Modèle OpenAI par défaut
OPENAI_MODEL=gpt-4o-mini

# Modèle d'embedding par défaut
EMBEDDING_MODEL=text-embedding-3-small

# === CONFIGURATION APPLICATION ===
# Nom de l'application
APP_NAME=PuppyCompanion

# Mode debug
DEBUG=true

# Niveau de log
LOG_LEVEL=INFO

# === CONFIGURATION CHAINLIT ===
# Port pour Chainlit (si utilisé)
CHAINLIT_PORT=8000

# Host pour Chainlit
CHAINLIT_HOST=localhost

# === CONFIGURATION JUPYTER ===
# Port pour Jupyter Lab
JUPYTER_PORT=8888

# === CHEMINS DE DONNÉES ===
# Chemin vers les données préprocessées
DATA_PATH=../puppycompanion-app/data/preprocessed_chunks.json

# Répertoire de travail temporaire
TEMP_DIR=/tmp/puppycompanion_dev

# === CONFIGURATION DÉVELOPPEMENT ===
# Environnement
ENVIRONMENT=development

# Version Python
PYTHON_VERSION=3.12

# Nom de l'environnement virtuel
VENV_NAME=venv_dev_312 